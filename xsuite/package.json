{
  "name": "xsuite",
  "version": "0.0.98",
  "license": "MIT",
  "bin": {
    "xsuite": "cli.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "build-wasms": "xsuite build-reproducible contracts -rd --image multiversx/sdk-rust-contract-builder:v8.0.0",
    "verify-wasms": "pnpm build-wasms && (git status | grep -q \\\\.wasm && exit 1 || exit 0)",
    "test": "vitest run",
    "open-coverage": "open coverage/index.html",
    "try": "tsx scripts/try.ts"
  },
  "files": [
    "assert.d.ts",
    "assert.js",
    "bin",
    "cli.d.ts",
    "cli.js",
    "data.d.ts",
    "data.js",
    "dist",
    "index.d.ts",
    "index.js",
    "interact.d.ts",
    "interact.js",
    "proxy.d.ts",
    "proxy.js",
    "world.d.ts",
    "world.js"
  ],
  "dependencies": {
    "@multiversx/sdk-core": "13.13.1",
    "@multiversx/sdk-native-auth-client": "1.0.9",
    "@multiversx/sdk-wallet": "4.6.0",
    "@xsuite/full-simulnet": "workspace:*",
    "@xsuite/light-simulnet": "workspace:*",
    "bech32": "2.0.0",
    "chalk": "4.1.2",
    "commander": "12.0.0",
    "glob": "^11.0.0",
    "open": "8.4.2",
    "protobufjs": "7.4.0",
    "tar": "7.4.3",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "22.8.6",
    "@types/tar": "6.1.13",
    "@vitest/coverage-istanbul": "^2.1.4",
    "msw": "2.6.0",
    "rimraf": "6.0.1",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "undici": "^6.20.1",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
