{
  "name": "xsuite",
  "version": "0.0.18",
  "license": "MIT",
  "scripts": {
    "build": "pnpm build-fproxy && pnpm build-lib",
    "build-fproxy": "run-script-os",
    "build-fproxy:linux": "cd fproxy && GOOS=linux GOARCH=amd64 LIBRARY_PATH=$(pwd)/../bin go build -o ../bin/fproxy-linux-amd64 -ldflags \"-extldflags '-Wl,-rpath,\\$ORIGIN'\"",
    "build-fproxy:darwin": "cd fproxy && GOOS=darwin GOARCH=amd64 go build -o ../bin/fproxy-darwin-amd64",
    "build-lib": "rimraf ./dist && tsc",
    "build-contracts": "INIT_CWD=contracts pnpm xsuite build --locked --target-dir $PWD/contracts/target",
    "test": "jest lib/",
    "open-coverage": "open ./coverage/lcov-report/index.html",
    "try": "tsx scripts/try.ts"
  },
  "files": [
    "bin",
    "dist",
    "data.js",
    "data.d.ts",
    "proxy.js",
    "proxy.d.ts",
    "test.js",
    "test.d.ts",
    "world.js",
    "world.d.ts"
  ],
  "dependencies": {
    "@multiversx/sdk-wallet": "4.2.0",
    "bech32": "2.0.0",
    "protobufjs": "7.0.0"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@swc/core": "1.3.46",
    "@swc/jest": "0.2.24",
    "@types/node": "18.7.18",
    "jest": "29.5.0",
    "open": "^9.1.0",
    "rimraf": "5.0.0",
    "run-script-os": "^1.1.6",
    "tmp-promise": "^3.0.3",
    "tsx": "^3.12.7",
    "typescript": "5.0.4"
  }
}
