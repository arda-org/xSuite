{
  "name": "xsuite-cli",
  "version": "0.0.6",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && ncc build ./src/index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "xsuite": "index.js"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "devDependencies": {
    "@multiversx/sdk-core": "12.2.0",
    "@multiversx/sdk-native-auth-client": "1.0.2",
    "@types/node": "18.7.18",
    "@types/tar": "6.1.5",
    "@vercel/ncc": "0.36.1",
    "chalk": "4.1.2",
    "commander": "10.0.1",
    "rimraf": "5.0.0",
    "tar": "6.1.15",
    "typescript": "5.0.4",
    "xsuite": "workspace:*"
  }
}
